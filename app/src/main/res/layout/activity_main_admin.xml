<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Admin.MainActivityAdmin"
    android:windowSoftInputMode="adjustResize">

    <FrameLayout
        android:id="@+id/frame_layout_admin"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/btnNavAdmin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/btnNavAdmin"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/icons_background"
        app:itemBackground="@drawable/custom_bottom_nav_item_selector"
        app:itemIconTint="@drawable/custom_bottom_nav_item_color"
        app:itemTextColor="@drawable/custom_bottom_nav_item_color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/boton_nav_admin" />

</androidx.constraintlayout.widget.ConstraintLayout>